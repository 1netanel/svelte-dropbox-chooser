{
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "useTabs": false,
  "printWidth": 90,
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "svelteSortOrder": "scripts-markup-options-styles",
  "svelteStrictMode": false,
  "svelteIndentScriptAndStyle": true,
  "arrowParens": "avoid",
  "trailingComma": "none"
}